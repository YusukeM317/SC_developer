  "source": "pragma solidity 0.7.0;\n\ncontract DeactivationAntipattern {\n    address payable owner;\n\n    constructor() {\n      owner = msg.sender;\n    }\n\n    modifier onlyOwner(){\n        require (owner == msg.sender, \"Not authorized\");\n        _;\n    }\n\n    receive() external payable {\n    }\n    \n    function close() public onlyOwner { \n      selfdestruct(msg.sender); \n    }\n\n}",
